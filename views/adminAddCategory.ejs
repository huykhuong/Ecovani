<%- include('partials/adminHeader') %>
<h2 class="page-title">Add a Category</h2>
<a href="/admin/category" class="btn btn-primary">Back to all categories</a>
<br><br>
<%- include('partials/messages') %>
<form action="/admin/category/add-category" method="post">
  <div class="form-group">
    <label for="">Title</label>
    <input type="text" class="form-control" name="title" value="<%=title%>" placeholder="Title">
  </div>
  <button style="border: 1px solid #000" class="btn btn-default" type="submit">Submit</button>
</form>
<%- include('partials/adminFooter') %>
